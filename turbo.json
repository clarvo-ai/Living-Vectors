{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false
    },
    "db:migrate": {
      "env": ["DATABASE_URL", "DIRECT_DATABASE_URL"],
      "cache": false,
      "persistent": true
    },
    "db:deploy": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    }
  }
}
